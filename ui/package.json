    "ahooks": "^3.7.8",
    "ansi_up": "^5.2.1",
    "antd": "^5.11.3",
    "axios": "^1.3.5",
    "bignumber.js": "^9.1.2",
    "bizcharts": "^4.1.22",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "highlight.js": "^11.9.0",
    "js-yaml": "^4.1.0",
